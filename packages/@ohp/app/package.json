    "babel-preset-env": "^1.5.2",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-register": "^6.24.1",
    "electron": "^1.6.11",
    "electron-builder": "^19.6.1",
    "electron-devtools-installer": "^2.2.0",
